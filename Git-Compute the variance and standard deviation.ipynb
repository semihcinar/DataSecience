   "outputs": [],
   "source": [
    "import numpy as np\n",
    "import pandas as pd\n",
    "from scipy import stats\n",
    "samples = np.random.normal(loc= 100, scale = 15,size=1000)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "id": "cdd59723",
   "metadata": {},
   "outputs": [],
   "source": [
